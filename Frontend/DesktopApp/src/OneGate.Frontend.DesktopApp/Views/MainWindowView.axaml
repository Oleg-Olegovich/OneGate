<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:OneGate.Frontend.DesktopApp.ViewModels;assembly=OneGate.Frontend.DesktopApp"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="OneGate.Frontend.DesktopApp.Views.MainWindowView"
        Icon="/Assets/onegate-logo.ico"
        Content="{Binding Content}"
        Width="800"
        Height="600"
        MinWidth="500"
        MinHeight="500"
        SizeToContent="WidthAndHeight"
        WindowStartupLocation="CenterScreen"
        Title="OneGate.Frontend.DesktopApp">

    <Design.DataContext>
        <vm:MainWindowViewModel />
    </Design.DataContext>

</Window>